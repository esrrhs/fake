func isprime(n)
	nn := n - 1
    for var i = 2 -> nn, 1 then
        if not (n % i) then
            return false
        end
    end
    return true
end


func primes(n)
    var count = 0
    for var i = 2 -> n, 1 then
        if is isprime(i) then
            count++
        end
    end
    return count
end

func main()

    var a = primes(10 * 10000)
    print(a)

    return a
 
end
